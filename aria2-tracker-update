@echo off
echo Updating Trackers ...
curl https://cdn.staticaly.com/gh/XIU2/TrackersListCollection/master/all_aria2.txt -o trackers.txt
findstr /b /v "bt-tracker=" "aria2.conf" > "aria2.conf.tmp"
for /f "delims=" %%a in (trackers.txt) do echo bt-tracker=%%a >> "aria2.conf.tmp"
move /y "aria2.conf.tmp" "aria2.conf" >nul 2>nul
del trackers.txt
